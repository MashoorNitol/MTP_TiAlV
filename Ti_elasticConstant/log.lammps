variable natoms equal "count(all)"
variable vol_atom equal "vol/v_natoms"
variable ecoh equal "pe/v_natoms"
variable LX0 equal "lx"
variable LY0 equal "ly"
variable LZ0 equal "lz"
variable CArat equal "v_LZ0/v_LX0"
#include potential-mod.mod
# Compute initial state
fix 3 all box/relax aniso 0.0 vmax 0.01
minimize ${etol} ${ftol} ${maxiter} ${maxeval}
minimize 1e-12 ${ftol} ${maxiter} ${maxeval}
minimize 1e-12 1e-12 ${maxiter} ${maxeval}
minimize 1e-12 1e-12 100000 ${maxeval}
